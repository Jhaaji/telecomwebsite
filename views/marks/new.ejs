<% include ../partials/header %>

<div class="container">



<form action="/marks/new" method="POST">
    <div class="form-group">
    	<label>Semester</label>
        <input  type="number" name="semester" min ="1" max="8">
    </div>
    <div class="form-group">
    	<label>Code</label>
        <input class="form-control" type="text" name="code" min ="1" max="8">
    </div>
     <div class="form-group">
    	<label>Merit</label>
        <input  type="number" name="merit" step="0.5" min ="1" max="5">
    </div>
    <div class="form-group">
    	<label>Grade</label>
        <input class="form-control" type="text" name="grade" min ="1" max="8">
    </div>

    <button class="btn btn-lg btn-default"> Submit </button>
    </div>
</form>

</div>

<% include ../partials/footer %>